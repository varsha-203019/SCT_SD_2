#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main() {
    int num, guess;

    srand(time(0));            // generate random number
    num = rand() % 10 + 1;     // number between 1 and 10

    printf("Guess a number between 1 and 10\n");

    do {
        printf("Enter your guess: ");
        scanf("%d", &guess);

        if (guess > num)
            printf("Too high! Try again.\n");
        else if (guess < num)
            printf("Too low! Try again.\n");
        else
            printf("ðŸŽ‰ Correct! You guessed the number.\n");

    } while (guess != num);

    return 0;
}

